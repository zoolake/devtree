+  background-color: #f0f0f0;
+  border-bottom-width: 0;
+  border-left-width: 0;
+  left: -5px;
+}
+
+/* End message from other user */
+
+/* Start my messages */
+
+.message.right .msg-detail .msg-info {
+  text-align: right;
